      <!--First slide-->
      <div class="carousel-item active">

        <div class="col-md-4" style="float:left">
          <div class="card mb-2">
            {{#if user}}
            <a href="/puzzle">
              <div class="card-body">
                <h1 class="card-title">Puzzle</h1>
