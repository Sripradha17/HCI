                <img class="card-img-top" src="../../../public/images/puzzle.jpg"
                  alt="Card image cap">
              </div>
            </a>
            {{/if}}
          </div>
        </div>

        <div class="col-md-4" style="float:left">
          <div class="card mb-2">
            {{#if user}}
            <a href="/racing">
